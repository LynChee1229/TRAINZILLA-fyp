!\");\r\n        }\r\n    }\r\n\r\n    return (\r\n        <>\r\n        <Header/>\r\n        <div>\r\n            <h1>Login Page</h1>\r\n\r\n            {/* you need this dangerMsg for show error message */}\r\n            <div className=\"dangerMsg alert alert-danger col-6 offset-3 d-none\"></div>\r\n\r\n            <input type=\"text\" value={key} onChange={(e)=>setKey(e.target.value)}  placeholder=\"User Name / Email\" required/> <br/><br/>\r\n            <input type=\"password\" value={userPassword} onChange={(e)=>setPassword(e.target.value)}  placeholder=\"Password\" required/> <br/><br/>\r\n            <button onClick={login} className=\"btn btn-primary\">Sign In</button> <br/><br/>\r\n        </div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Login","C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\MapRoute.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Announcement.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\AboutUs.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Profile.js",["61"],"import Header from './Header'\r\nimport React, {useEffect} from \"react\"\r\nimport { useHistory } from 'react-router-dom'\r\n\r\nfunction Profile()\r\n{\r\n    useEffect(()=>{\r\n        if(!localStorage.getItem(\"user-info\")) {\r\n            history.push('/login');\r\n        }\r\n    }, []);\r\n    const history = useHistory();\r\n\r\n    return (\r\n        <>\r\n        <Header/>\r\n        <div>\r\n            <h1>Profile Page</h1>\r\n        </div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Profile","C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Home.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Bar.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Footer.js",[],"C:\\Users\\60163\\Desktop\\TRAINZILLA\\trainzilla-frontend\\src\\Register.js",["62"],"import Header from './Header'\r\nimport React,